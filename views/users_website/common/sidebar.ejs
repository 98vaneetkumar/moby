<div
  class="main-menu menu-fixed menu-light menu-accordion menu-shadow"
  data-scroll-to-active="true"
  style="background: linear-gradient(to right, #2a2a2a, #555)"
>
  <div class="navbar-header">
    <ul class="nav navbar-nav flex-row">
      <li class="nav-item mx-auto w-100">
        <a class="navbar-brand me-0 w-100" href="/admin/dashboard">
          <span class="brand-logo w-100">
            <img
              src="../../assets/logo1.jpg"
              alt=""
              style="
                border-radius: 50%;
                width: 50px;
                height: 50px;
                margin-left: -7px;
                object-fit: cover;
                transition: all 0.3s ease;
              "
              class="sidebar-logo"
            />
          </span>
        </a>
      </li>

      <li class="nav-item nav-toggle">
        <a class="nav-link modern-nav-toggle pe-0" data-bs-toggle="collapse"
          ><i
            class="d-block d-xl-none text-primary toggle-icon font-medium-4"
            data-feather="x"
          ></i
          ><i
            class="d-none d-xl-block collapse-toggle-icon font-medium-4 text-primary"
            data-feather="disc"
            data-ticon="disc"
          ></i
        ></a>
      </li>
    </ul>
  </div>
  <div class="main-menu-content">
    <ul
      class="navigation navigation-main"
      id="main-menu-navigation"
      data-menu="menu-navigation"
    >
      <li
        class="<% if(title =='Dashboard'){ %> nav-item dropdown active <% }%>"
      >
        <a class="d-flex align-items-center" href="/admin/dashboard"
          ><i class="fas fa-home"></i>
          <span class="menu-title text-truncate" data-i18n="Dashboards"
            >Dashboard</span
          ></a
        >
      </li>
      <li class="<% if(title =='Users'){ %> nav-item dropdown active <% }%>">
        <a class="d-flex align-items-center" href="/admin/users_listing">
          <i class="fas fa-users"></i>
          <span class="menu-title text-truncate" data-i18n="Users"
            >Users</span
          ></a
        >
      </li>

      <li class="<% if(title =='Patients/Users'){ %> nav-item dropdown active <% }%>">
        <a class="d-flex align-items-center" href="/admin/patients_listing">
          <i class="fas fa-user-injured"></i>
          <span class="menu-title text-truncate" data-i18n="Patients">Patients</span>
        </a>
      </li>
      
      <li class="<% if(title =='Drivers'){ %> nav-item dropdown active <% }%>">
        <a class="d-flex align-items-center" href="/admin/drivers_listing">
          <i class="fas fa-car"></i>
          <span class="menu-title text-truncate" data-i18n="Drivers">Drivers</span>
